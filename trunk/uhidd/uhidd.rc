#!/bin/sh
#
# $FreeBSD$
#

# PROVIDE: uhidd
# REQUIRE: DAEMON cleanvar
# KEYWORD: nojail shutdown

. /etc/rc.subr

name="uhidd"
rcvar=`set_rcvar`
command="/usr/local/sbin/${name}"
start_cmd="uhidd_start"
pidprefix="/var/run/uhidd"

load_rc_config $name

if [ -n "$2" ]; then
	pidfile="${pidprefix}.$2.pid"
else
	pidfile=
fi

uhidd_start()
{
	/usr/local/sbin/uhidd ${uhidd_flags} $1
}

run_rc_command $*
